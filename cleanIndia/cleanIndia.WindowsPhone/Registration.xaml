<Page
    x:Class="cleanIndia.Registration"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:cleanIndia"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBox x:Name="uname" HorizontalAlignment="Left" Margin="57,102,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <TextBox x:Name="email" HorizontalAlignment="Left" Margin="57,159,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <TextBox x:Name="phone" HorizontalAlignment="Left" Margin="57,229,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="57,306,0,0" VerticalAlignment="Top" Width="89"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="37,392,0,0" VerticalAlignment="Top" Click="RegisterButton_Click"/>
        <TextBlock x:Name="test" HorizontalAlignment="Left" Margin="115,63,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>

    </Grid>
</Page>
